{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/localtunnel/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/localtunnel",
        "main": "packages/localtunnel/src/cli.ts",
        "tsConfig": "packages/localtunnel/tsconfig.lib.json",
        "assets": ["packages/localtunnel/*.md"]
      }
    },
    "start": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "localtunnel:build",
        "args": [
          "--port=8091",
          "--host=http://localhost:8089",
          "--debug",
          "--print-requests"
        ]
      },
      "configurations": {
        "production": {
          "args": [
            "--host=http://tunnel.monday.app",
            "--debug",
            "--print-requests"
          ]
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/localtunnel/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/localtunnel"],
      "options": {
        "jestConfig": "packages/localtunnel/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "push": true,
        "postTargets": ["localtunnel:github", "localtunnel:publish"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  },
  "tags": []
}
